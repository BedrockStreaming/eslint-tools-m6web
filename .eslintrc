{
  "extends": [
    "m6web",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    // prettier config
    "prettier/prettier": ["error",
      {
        "singleQuote": true,
        "printWidth": 120,
        "trailingComma": "all"
      }
    ]
  }
}
